    "stop": "cd dest && forever stop app.js;",
    "kill": "pkill -f selenium-standalone",
    "postinstall": "gulp build",
    "build": "gulp build && sh ./make.zip.4.release.chrome.extension.sh"
  },
  "engines": {
    "node": "5.4.1",
    "npm": "3.3.12"
  },
