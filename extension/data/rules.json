[
	{
		"rule-id": 1,
		"title": "Absoluted(Fixed) Element",
		"base-styles": {
			"position": "[absolute|fixed]"
		},
		"ng-styles": {
			"warning-styles": {
				"display": [
					"!default",
					"Display property is needless. Defined display property has changed to block(or list-item) forcibly. So this display property is needless.",
					""
				],
				"float": [
					"!default",
					"Needless. Defined property is no effective.",
					""
				]
			}
		}
	},
