<figure class="result cf">
	<figcaption class="result-body">
		<table class="result-table">
			<tr>
				<th>Total: </th><td>{{total}}</td>
			</tr>
			<tr>
				<th>Error: </th><td>{{error}}</td>
			</tr>
			<tr>
				<th>Warning: </th><td>{{warning}}</td>
			</tr>
		</table>
	</figcaption>
	<div class="result-screenshot">
		<figure class="result-screenshot-display">
			<img src="{{screenshot}}" alt="Screenshot" class="screenshot-image" />
		</figure>
	</div>
</figure>